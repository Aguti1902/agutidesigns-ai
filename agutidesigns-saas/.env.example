# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
# Opcional: si no pones VITE_API_URL, se usa VITE_SUPABASE_URL + /functions/v1
VITE_API_URL=https://your-project.supabase.co/functions/v1

# Stripe (clave pública para el frontend; las secretas van en Supabase Edge Functions)
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_xxx
# En Supabase Dashboard > Edge Functions > Secrets añade:
#   STRIPE_SECRET_KEY = sk_test_xxx (o sk_live_xxx)
#   STRIPE_WEBHOOK_SECRET = whsec_xxx (del webhook en Stripe Dashboard)

# OpenAI (para prompt builder)
VITE_OPENAI_API_KEY=sk-xxx
